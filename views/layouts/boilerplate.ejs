<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/bs/bootstrap.min.css">
    <link rel="stylesheet" href="/style/boilerplate.css">
    <link rel="stylesheet" href="/style/partial/footer.css">
    <link rel="icon" href="/img/favicon.svg">
</head>
<body>
    <div class="container main-container p-0 d-flex flex-column align-items-center">
      <div class="row" id="imageheader">
        <div class="col py-2">
          <a href="/"><img src="/img/logo.png" alt=""></a>
        </div>
      </div>
      <div class="row">
        <div class="col p-0">
          <%- include("../partials/navbar") %>
        </div>
      </div>
      <div class="row justify-content-center mb-3" id="content-row">
        <div class="col-11" id="content-column">
          <%- include("../partials/flash") %>
          <%- body %>
        </div>
      </div>
      <div class="row mt-auto justify-content-center" id="bottomrow">
        <div class="col-11">
          <%- include("../partials/footer") %>
        </div>
      </div>
    </div>
    <script>
        (function () {
        'use strict'
      
        // Fetch all the forms we want to apply custom Bootstrap validation styles to
        var forms = document.querySelectorAll('.needs-validation')
      
        // Loop over them and prevent submission
        Array.prototype.slice.call(forms)
          .forEach(function (form) {
            form.addEventListener('submit', function (event) {
              if (!form.checkValidity()) {
                event.preventDefault()
                event.stopPropagation()
              }
      
              form.classList.add('was-validated')
            }, false)
          })
      })()
      </script> 
    <script src="/bs/bootstrap.min.js"></script>
</body>
</html>
<script src="/js/popover.js"></script>
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
